            },
            {
                "language": "typescript",
                "path": "./snippets/typescript.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/vitest.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/react.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/remix.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/typescript.json"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/vitest.json"
            },
            {
                "language": "markdown",
                "path": "./snippets/markdown.json"
