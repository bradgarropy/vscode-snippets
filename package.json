        "snippets",
        "javascript",
        "typescript",
        "vitest",
        "react",
        "markdown",
        "remix"
    ],
    "homepage": "https://marketplace.visualstudio.com/items?itemName=bradgarropy.bg-snippets",
    "bugs": {
        "url": "https://github.com/bradgarropy/vscode-snippets/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Brad Garropy",
        "email": "bradgarropy@gmail.com",
        "url": "https://bradgarropy.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bradgarropy/vscode-snippets"
    },
    "scripts": {
        "format": "prettier --check .",
        "format:fix": "prettier --write ."
    },
    "devDependencies": {
        "prettier": "^3.4.2"
    },
    "publisher": "bradgarropy",
    "engines": {
