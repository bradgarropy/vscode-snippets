/*
	// Place your snippets for Python here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "log",
		"body": [
			"console.log('$1');",
			"$2"
		],
		"description": "Log output to console"
	}
*/

	"Console Log": {
		"prefix": 	   "log",
		"body": 	   "logging.info($1)",
		"description": "Logs an info message to the console."
	},
	"Console Log - Info": {
		"prefix": 	   "logi",
		"body": 	   "logging.info($1)",
		"description": "Logs an info message to the console."
	},
	"Console Log - Warning": {
		"prefix": 	   "logw",
		"body": 	   "logging.warning($1)",
		"description": "Logs a warning message to the console."
	},
	"Console Log - Error": {
		"prefix": 	   "loge",
		"body": 	   "logging.error($1)",
		"description": "Logs an error message to the console."
	},
	"Method Definition": {
		"prefix": 	   "def",
		"body": 	   ["def $1():",
						"    \"\"\" $2",
						"",
						"    Parameters:",
						"        None",
						"",
						"    Returns:",
						"        None",
						"    \"\"\"",
						"",
						"    return"],
		"description": "Method definition with docstring."
	}
}